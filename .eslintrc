{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "no-use-before-define": [
            "error",
            { "functions": false, "classes": true }
        ]
    }
}
